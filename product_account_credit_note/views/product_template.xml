<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="product_template_form_view_account" model="ir.ui.view">
        <field name="name">product.template.form.account</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="account.product_template_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_income_id']" position="after">
                <field name="credit_note_account_id" />
            </xpath>
        </field>
    </record>
</odoo>
