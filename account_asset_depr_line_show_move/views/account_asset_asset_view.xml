<openerp>
    <data>
        <record id="view_account_asset_asset_form" model="ir.ui.view">
            <field name="name">Account Asset Form (cancel button)</field>
            <field name="model">account.asset.asset</field>
            <field name="inherit_id" ref="account_asset.view_account_asset_asset_form"/>
            <field name="arch" type="xml">
                <button name="create_move" position="after">
                    <button name="open_move"
                            icon="STOCK_REDO"
                            string="View Move"
                            type="object"
                            attrs="{'invisible': [('move_check', '=', False)]}"
                    />
                </button>
            </field>
        </record>
    </data>
</openerp>
