<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="wizard_rebuild_moves_products" model="ir.ui.view">
        <field name="name">Re-booking Picking Entries</field>
        <field name="model">wizard.rebuild.moves.products</field>
        <field name="arch" type="xml">
            <form string="Product templates" >
                <group>
                    <group>
                        <field name="company_id"/>
                    </group>
                </group>
                <footer>
                    <button name="rebuild_moves_server" class="btn-primary" string="Next" type="object"/>
                    <button special="cancel" string="Cancel" class="btn-default"/>
                </footer>
            </form>
        </field>
    </record>

   <act_window name="Re-booking Picking Entries"
        res_model="wizard.rebuild.moves.products"
        src_model="product.template"
        view_mode="form"
        target="new"
        key2="client_action_multi"
        id="action_wizard_rebuild_moves_products"
        view_id="wizard_rebuild_moves_products"/>
</odoo>
