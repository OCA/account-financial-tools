<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="account_asset_view_form" model="ir.ui.view">
        <field name="name">account.asset.form</field>
        <field name="model">account.asset</field>
        <field
            name="inherit_id"
            ref="account_asset_management.account_asset_view_form"
        />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <field name="domain_tag_ids" />
            </xpath>
            <xpath expr="//field[@name='analytic_tag_ids']" position="attributes">
                <attribute
                    name="domain"
                >['|', ('id', 'in', domain_tag_ids or []), ('analytic_dimension_id.by_sequence', '=', False)]</attribute>
            </xpath>
        </field>
    </record>
</odoo>
