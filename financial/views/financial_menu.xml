<odoo>

    <record model="ir.ui.menu" id="financial_top_menu">
        <field name="name">Financial</field>
        <field name="sequence" eval="10"/>
    </record>

    <record model="ir.ui.menu" id="financial_receivable_menu">
        <field name="name">Receivables</field>
        <field name="parent_id" ref="financial_top_menu"/>
        <field name="sequence" eval="16"/>
    </record>
    <record model="ir.ui.menu" id="financial_payable_menu">
        <field name="name">Payables</field>
        <field name="parent_id" ref="financial_top_menu"/>
        <field name="sequence" eval="16"/>
    </record>

    <record model="ir.ui.menu" id="financial_report_menu">
        <field name="name">Reports</field>
        <field name="parent_id" ref="financial_top_menu"/>
        <field name="sequence" eval="46"/>
    </record>

    <record model="ir.ui.menu" id="financial_config_menu">
        <field name="name">Configuration</field>
        <field name="parent_id" ref="financial_top_menu"/>
        <field name="sequence" eval="96"/>
    </record>

</odoo>
