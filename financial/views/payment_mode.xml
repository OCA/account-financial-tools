<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_payment_mode_financial_form" model="ir.ui.view">
        <field name="name">account.payment.mode.form</field>
        <field name="model">account.payment.mode</field>
        <field name="inherit_id"
               ref="account_payment_mode.account_payment_mode_form"/>
        <field name="arch" type="xml">
            <field name="payment_type" position="after">
                <field name="liquidity" type="checkbox"/>
                <field name="interest_percent"/>
                <field name="delay_fee_percent"/>
            </field>
        </field>
    </record>
</odoo>