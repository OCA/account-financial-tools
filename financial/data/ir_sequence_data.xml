<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_financial_move_receivable" model="ir.sequence">
            <field name="name">Financial Receivable</field>
            <field name="code">financial.move.receivable</field>
            <field name="prefix">FR/</field>
            <field name="padding">6</field>
            <field name="company_id" eval=""/>
        </record>

        <record id="seq_financial_move_payable" model="ir.sequence">
            <field name="name">Financial Payable</field>
            <field name="code">financial.move.payable</field>
            <field name="prefix">FP/</field>
            <field name="padding">6</field>
            <field name="company_id" eval=""/>
        </record>

        <record id="seq_financial_move_receipt" model="ir.sequence">
            <field name="name">Financial Receipt</field>
            <field name="code">financial.move.receipt</field>
            <field name="prefix">RR/</field>
            <field name="padding">6</field>
            <field name="company_id" eval=""/>
        </record>

        <record id="seq_financial_move_payment" model="ir.sequence">
            <field name="name">Financial Payment</field>
            <field name="code">financial.move.payment</field>
            <field name="prefix">PP/</field>
            <field name="padding">6</field>
            <field name="company_id" eval=""/>
        </record>

    </data>
</odoo>
